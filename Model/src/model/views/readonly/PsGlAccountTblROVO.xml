<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PsGlAccountTblROVO"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.views.readonly.PsGlAccountTblROVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT A.ACCOUNT, 
  A.DESCRIPTION 
FROM PS_GL_ACCOUNT_TBL A 
WHERE ( A.EFFECTIVE_DATE = 
  (SELECT MAX(A_ED.EFFECTIVE_DATE) 
  FROM PS_GL_ACCOUNT_TBL A_ED 
  WHERE A.SETID            = A_ED.SETID 
  AND A.ACCOUNT            = A_ED.ACCOUNT 
  AND A_ED.EFFECTIVE_DATE <= SYSDATE 
  ) 
AND A.SETID              = 'MASTR' 
AND A.STATISTICS_ACCOUNT = 'Y' 
AND A.EFFECTIVE_STATUS   = 'A')]]></SQLQuery>
  <ViewAttribute
    Name="Account"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ACCOUNT"
    Expression="ACCOUNT"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="model.views.readonly.PsGlAccountTblROVO.Account_LABEL"/>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Description"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DESCRIPTION"
    Expression="DESCRIPTION"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="model.views.readonly.PsGlAccountTblROVO.Description_LABEL"/>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="Account"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
