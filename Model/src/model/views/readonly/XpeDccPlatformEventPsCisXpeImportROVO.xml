  <SQLQuery><![CDATA[SELECT 
    xpe_contract_id, 
    xpe_event_nbr, 
    xpe_event_dttm, 
    xpe_event_type, 
    xpe_carrier_id, 
    xpe_vehicle_nbr, 
    cis_exec_plan_step, 
    cis_stage, 
    attr_value9, 
    attr_value8, 
    attr_value1, 
    attr_value2, 
    cis_trans_bi_amt3, 
    rn 
FROM 
    ( 
        SELECT 
            plt_frm.xpe_contract_id, 
            plt_frm.xpe_event_nbr, 
            plt_frm.xpe_event_dttm, 
            plt_frm.xpe_event_type, 
            plt_frm.xpe_carrier_id, 
            plt_frm.xpe_vehicle_nbr, 
            xpe_imprt.cis_exec_plan_step, 
            xpe_imprt.cis_stage, 
            xpe_imprt.attr_value9, 
            xpe_imprt.attr_value8, 
            xpe_imprt.attr_value1, 
            xpe_imprt.attr_value2, 
            xpe_imprt.cis_trans_bi_amt3, 
            ROW_NUMBER() OVER( 
                PARTITION BY plt_frm.xpe_event_nbr 
                ORDER BY 
                    plt_frm.xpe_event_nbr DESC 
            ) rn 
        FROM 
            xpe_dcc_platform_event plt_frm, 
            ps_cis_xpe_import xpe_imprt 
        WHERE 
            plt_frm.xpe_event_nbr = xpe_imprt.key2 
            AND   plt_frm.xpe_contract_id = xpe_imprt.cis_targtcust3det1 
            AND   plt_frm.xpe_contract_version = xpe_imprt.cis_targtcust3det2 
            AND   plt_frm.xpe_contract_line = xpe_imprt.cis_targtcust3det3 
            AND   xpe_imprt.cis_exec_plan_step NOT LIKE 'DRV%' 
            AND   xpe_imprt.cis_stage LIKE 'FIN' 
            AND   plt_frm.xpe_event_dttm >= SYSDATE - 90
    ) 
WHERE 
    rn = 1]]></SQLQuery>
