  <SQLQuery><![CDATA[SELECT          
E.XPE_FACILITY,         
D.XPE_CONTRACT_ID,         
D.XPE_DCC_PCSNAME,         
A.CUST_ID,         
A.Name1 CUST_NAME,         
F.XPE_CONTRACT_VERSION,         
F.XPE_END_DATE,         
E.XPE_PRODUCT_ID,         
F.SALES_PERSON,  
B.ADDRESS_SEQ_NUM,    
E.XPE_CONTRACT_LINE    
FROM PS_CUSTOMER A, PS_CUST_ADDR_SEQ B,                                  
PS_CUST_ADDRESS C, XPE_DCC_CONTRACTS D,XPE_DCC_CONTRACT_LINE E ,XPE_DCC_CONTRACT_VERSION F                        
WHERE A.SETID='MASTR'          
AND B.BILL_TO_ADDR='Y'          
AND C.LANGUAGE_CD='ENG'          
AND F.XPE_CONTRACT_STATUS = 'APR'          
AND A.CUST_ID=B.CUST_ID                                                                               
AND B.CUST_ID=C.CUST_ID                                                                                             
AND A.CUST_ID=D.CUST_ID          
AND B.ADDRESS_SEQ_NUM=C.ADDRESS_SEQ_NUM          
AND C.ADDRESS_SEQ_NUM=D.ADDRESS_SEQ_NUM          
AND D.XPE_CONTRACT_ID=F.XPE_CONTRACT_ID           
AND F.XPE_CONTRACT_ID=E.XPE_CONTRACT_ID                         
AND F.XPE_CONTRACT_VERSION=E.XPE_CONTRACT_VERSION]]></SQLQuery>
