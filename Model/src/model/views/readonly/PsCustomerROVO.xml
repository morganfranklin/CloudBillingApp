<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PsCustomerROVO"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.views.readonly.PsCustomerROVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT A.CUST_ID,   
  A.NAME1 
FROM PS_CUSTOMER A,   
  PS_CUST_ADDR_SEQ B,   
  PS_CUST_ADDRESS C  
WHERE A.SETID             ='SHARE'   
AND A.SETID               =B.SETID   
AND A.CUST_ID             =B.CUST_ID   
AND B.BILL_TO_ADDR        ='Y'   
AND B.SETID               =C.SETID   
AND B.CUST_ID             =C.CUST_ID   
AND B.ADDRESS_SEQ_NUM     =C.ADDRESS_SEQ_NUM   
AND C.LANGUAGE_CD         ='ENG']]></SQLQuery>
  <ViewAttribute
    Name="CustId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="15"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CUST_ID"
    Expression="CUST_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="15"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="model.views.readonly.PsCustomerROVO.CustId_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Name1"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="NAME1"
    Expression="NAME1"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="model.views.readonly.PsCustomerROVO.Name1_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="CustId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
