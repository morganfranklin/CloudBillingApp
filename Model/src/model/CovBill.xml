<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
    xmlns="http://xmlns.oracle.com/bc4j"
    Name="CovBill"
    Version="12.2.1.0.31"
    InheritPersonalization="merge"
    BindingStyle="OracleName"
    CustomQuery="true"
    PageIterMode="Full">
    <DesignTime>
        <Attr Name="_isExpertMode" Value="true"/>
    </DesignTime>
    <Properties>
        <SchemaBasedProperties>
            <LABEL
                ResId="model.CovBill_LABEL"/>
        </SchemaBasedProperties>
    </Properties>
    <SQLQuery><![CDATA[SELECT SEQ_NUM, CHAR1 AS BILL_LINE_DESC, NUMBER1 AS LINE_AMOUNT 
FROM  
COV_CONTRACT_DETAILS 
WHERE 
DETAIL_CLASS='BILL' 
ORDER BY  
SEQ_NUM]]></SQLQuery>
    <ViewAttribute
        Name="SeqNum"
        IsUpdateable="false"
        IsPersistent="false"
        PrecisionRule="true"
        Type="java.lang.Integer"
        ColumnType="INTEGER"
        AliasName="SEQ_NUM"
        Expression="SEQ_NUM"
        SQLType="NUMERIC">
        <Properties>
            <SchemaBasedProperties>
                <LABEL
                    ResId="model.CovBill.SeqNum_LABEL"/>
            </SchemaBasedProperties>
        </Properties>
    </ViewAttribute>
    <ViewAttribute
        Name="BillLineDesc"
        IsUpdateable="false"
        IsPersistent="false"
        PrecisionRule="true"
        Precision="20"
        Type="java.lang.String"
        ColumnType="VARCHAR2"
        AliasName="BILL_LINE_DESC"
        Expression="BILL_LINE_DESC"
        SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="20"/>
        </DesignTime>
        <Properties>
            <SchemaBasedProperties>
                <LABEL
                    ResId="model.CovBill.BillLineDesc_LABEL"/>
            </SchemaBasedProperties>
        </Properties>
    </ViewAttribute>
    <ViewAttribute
        Name="LineAmount"
        IsUpdateable="false"
        IsPersistent="false"
        PrecisionRule="true"
        Precision="10"
        Scale="2"
        Type="java.math.BigDecimal"
