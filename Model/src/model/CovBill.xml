        Expression="LINE_AMOUNT"
        SQLType="NUMERIC">
        <Properties>
            <SchemaBasedProperties>
                <LABEL
                    ResId="model.CovBill.LineAmount_LABEL"/>
            </SchemaBasedProperties>
        </Properties>
    </ViewAttribute>
    <AttrArray Name="KeyAttributes">
        <Item Value="SeqNum"/>
    </AttrArray>
    <ResourceBundle>
        <PropertiesBundle
            PropertiesFile="model.ModelBundle"/>
    </ResourceBundle>
</ViewObject>
