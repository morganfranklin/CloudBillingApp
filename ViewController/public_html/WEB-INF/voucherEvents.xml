<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
    <task-flow-definition id="voucherEvents">
        <default-activity>voucherEventsView</default-activity>
        <data-control-scope>
            <shared/>
        </data-control-scope>
        <managed-bean id="__1">
            <managed-bean-name>auditReportsMBean</managed-bean-name>
            <managed-bean-class>view.AuditReportsMBean</managed-bean-class>
            <managed-bean-scope>pageFlow</managed-bean-scope>
        </managed-bean>
        <managed-bean id="__2">
            <managed-bean-name>auditReportsBBean</managed-bean-name>
            <managed-bean-class>view.auditReportsBBean</managed-bean-class>
            <managed-bean-scope>backingBean</managed-bean-scope>
        </managed-bean>
        <view id="voucherEventsView">
            <page>/voucherEvents.jsff</page>
        </view>
        <use-page-fragments/>
    </task-flow-definition>
</adfc-config>
